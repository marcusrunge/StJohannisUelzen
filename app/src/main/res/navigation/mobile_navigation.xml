<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_web">
    <fragment
        android:id="@+id/navigation_web"
        android:name="com.marcusrunge.stjohannisuelzen.ui.web.WebFragment"
        android:label="@string/title_web"
        tools:layout="@layout/web_fragment" />
    <fragment
        android:id="@+id/navigation_counseling"
        android:name="com.marcusrunge.stjohannisuelzen.ui.counseling.CounselingFragment"
        android:label="@string/title_counseling"
        tools:layout="@layout/counseling_fragment" />
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.marcusrunge.stjohannisuelzen.ui.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/navigation_licenses"
        android:name="com.marcusrunge.stjohannisuelzen.ui.licenses.LicensesFragment"
        android:label="licenses_fragment"
        tools:layout="@layout/licenses_fragment" />
    <fragment
        android:id="@+id/navigation_eula"
        android:name="com.marcusrunge.stjohannisuelzen.ui.eula.EulaFragment"
        android:label="eula_fragment"
        tools:layout="@layout/eula_fragment" />
    <fragment
        android:id="@+id/navigation_privacy"
        android:name="com.marcusrunge.stjohannisuelzen.ui.privacy.PrivacyFragment"
        android:label="privacy_fragment"
        tools:layout="@layout/privacy_fragment" />
</navigation>